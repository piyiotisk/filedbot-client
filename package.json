{
  "name": "fieldbot-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.2",
    "browser-image-compression": "^1.0.6",
    "heic2any": "0.0.2",
    "jsonwebtoken": "^8.5.1",
    "react": "^16.13.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1",
    "react-stripe-elements": "^6.1.0",
    "react-to-print": "^2.9.0",
    "uuid": "^3.4.0"
  },
  "scripts": {
    "build": "NODE_ENV='production' npm run build:css && react-scripts build",
    "build:css": "postcss src/index.css -o src/tailwind.css",
    "postbuild": "cp _redirects build/_redirects",
    "eject": "react-scripts eject",
    "start": "npm run build:css && PORT=5000 react-scripts start",
    "serve": "serve -s build",
    "test": "react-scripts test",
    "watch": "watch \"npm run build:css\" ./src"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@tailwindcss/ui": "^0.3.0",
    "@types/browser-image-compression": "^1.0.1",
    "@types/jest": "^26.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.13",
    "@types/react": "^16.9.36",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/uuid": "^8.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "install": "^0.13.0",
    "npm": "^6.14.5",
    "postcss": "^7.0.26",
    "postcss-cli": "^6.1.3",
    "react-test-renderer": "^16.13.1",
    "tailwindcss": "^1.4.5",
    "typescript": "^3.9.5"
  }
}
